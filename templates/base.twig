<!doctype html>
<!--[if lt IE 9]><html class="no-js no-svg ie lt-ie9 lt-ie8 lt-ie7" {{ site.language_attributes }}> <![endif]-->
<!--[if IE 9]><html class="no-js no-svg ie ie9 lt-ie9 lt-ie8" {{ site.language_attributes }}> <![endif]-->
<!--[if gt IE 9]><!-->
	<html
	class="no-js no-svg " {{ site.language_attributes }}> <!--<![endif]-->
	<head>
		<meta charset="{{ site.charset }}"/>
		<meta name="description" content="{{ site.description }}">
		<link rel="stylesheet" href="{{ site.theme.link }}/style.css?v=202105.11" type="text/css" media="screen"/>

		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<link rel="author" href="{{ site.theme.link }}/humans.txt"/>
		<link rel="pingback" href="{{ site.pingback_url }}"/>
		<link rel="profile" href="http://gmpg.org/xfn/11">
		{{function('wp_head')}}
		{% block head %}
			<meta property="og:title" value="{{ post.title }}"/>
		{% endblock %}
	</head>

	<body class="{{body_class}} overflow-x-hidde" data-template="base.twig">

		<header id="header">
			{% block header %}
				<div class="header-wrapper wrapper ">

					<a id="header_logo" class="hdr-logo-link" style="max-width: {{ custom_logo.max.width }}px; min-width: {{ custom_logo.min.width }}px;" href="{{ site.url }}" rel="home" title="{{ site.name }}">
						<img alt="EvMAR Automation" src="data:image/svg+xml;base64,"/>
					</a>

					<nav id="nav_main" class="nav-main flex-1" role="navigation">
						{% include "menu.twig" with {'items': menu.primary.get_items} %}
					</nav>
					<!-- #nav -->

					<nav id="nav_langs" class="nav-langs flex-grow-0" role="navigation">
						<ul class="navigation">
							{% for key, lang in langs %}
								<li class="nav-item {{ lang.current_lang ? 'is-current' : '' }}">
									{% if lang.current_lang %}
										<span class="h-full flex flex-wrap content-center px-1 uppercase font-bold text-gray-300">{{ key }}</span>
									{% else %}
										<a class="h-full flex flex-wrap content-center px-1 uppercase text-gray-300 hover:text-gray-500 focus:outline-none" href="{{ lang.url }}">{{ key }}</a>
									{% endif %}
								</li>
							{% endfor %}
						</ul>
					</nav>

					<!-- menu ico -->
					<button id="mobile_menu_toggle" class="mobile-menu-toggle ">
						<span class="text-transparent">Toggle Menu</span>
					</button>
				</div>
			{% endblock %}
		</header>

		<section id="content" role="main" class="content-wrapper -mt-24">
			{% if title %}
				<h1>{{title}}</h1>
			{% endif %}
			<div
				class="wrapper {{sidebar_class}}">
				<!-- twig-block: content -->
				{% block content %}
					Sorry, no content
				{% endblock %}
				<!-- //twig-block: content -->
			</div>
			{% if sidebar %}
				<aside class="layout-sidebar">
					{{sidebar}}
				</aside>
			{% endif %}
		</section>

		{% block footer %}
			<section id="footer_widgets">
				<div class="wrapper">
					{{ bottom_widgets }}
				</div>
			</section>
			<footer id="footer">
				{% include 'footer.twig' %}
			</footer>
			{{ function('wp_footer') }}
		{% endblock %}
	</body>
</html>
