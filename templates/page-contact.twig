{% extends "base.twig" %}

{% block content %}
	<!-- template: page-contact -->
	<div class="content-wrapper template-page-contact-wrapper bg-black pt-24"><!-- content wrapper -->
		<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
			<section class="article-content">
				<h1 class="article-h1" style="display: none;">{{post.title}}</h1>
				<div class="article-body">
					{% for section in post.meta('sections') %}
						{% include "partial/section.twig" with {'section': section, section_index: loop.index0} %}
					{% endfor %}
					{# {{post.content}} #}
				</div>
			</section>
		</article>
	</div><!-- //content-wrapper -->

	<!-- //template: page-contact -->
{% endblock %}
